{
  "name": "@socialgouv/emjpm-ui",
  "version": "0.0.1",
  "description": "Emjpm design system",
  "repository": "git@github.com:SocialGouv/emjpm-ui.git",
  "author": "kwiss <christophe@hooraylabs.com>",
  "license": "Apache-2.0",
  "files": [
    "dist"
  ],
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "lerna run build --parallel",
    "build-docs": "build-storybook --docs",
    "build-storybook": "build-storybook",
    "generate:component": "hygen component new --package components",
    "generate:core": "hygen component new --package core",
    "generate": "hygen component new",
    "chromatic": "CHROMATIC_APP_CODE=3zckn0l8m8w chromatic test",
    "lint": "eslint --fix . && echo 'Lint complete.'",
    "lint:fix": "eslint --fix packages/*",
    "lint:js": "cross-env NODE_ENV=production eslint --cache --cache-location=.cache/eslint --ext .js,.jsx,.html,.ts,.tsx,.mjs --report-unused-disable-directives",
    "lint:package": "sort-package-json",
    "storybook": "start-storybook -p 6006",
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "yarn lint:js --fix",
        "git add"
      ],
      "*.html": [
        "yarn lint:js --fix",
        "git add"
      ],
      "package.json": [
        "yarn lint:package",
        "git add"
      ]
    }
  },
  "dependencies": {
    "eslint-plugin-react-hooks": "1.7.0",
    "lerna": "3.16.4",
    "polished": "3.4.1",
    "rebass": "4.0.5"
  },
  "devDependencies": {
    "@babel/cli": "7.6.0",
    "@babel/core": "7.6.0",
    "@babel/node": "7.6.1",
    "@babel/plugin-proposal-object-rest-spread": "7.5.5",
    "@babel/plugin-transform-runtime": "7.6.0",
    "@babel/preset-env": "7.6.0",
    "@babel/preset-react": "7.0.0",
    "@emotion/core": "10.0.17",
    "@emotion/styled": "10.0.17",
    "@storybook/addon-a11y": "5.2.0-rc.11",
    "@storybook/addon-actions": "5.2.4",
    "@storybook/addon-docs": "5.2.0-rc.11",
    "@storybook/addon-knobs": "5.2.0-rc.11",
    "@storybook/addon-storysource": "5.2.0-rc.11",
    "@storybook/addons": "5.2.0-rc.11",
    "@storybook/react": "5.2.0-rc.11",
    "@storybook/source-loader": "5.2.0-rc.11",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "babel-plugin-require-context-hook": "1.0.0",
    "cross-env": "5.2.1",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-html": "5.0.5",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.17.0",
    "eslint-plugin-json": "1.4.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-simple-import-sort": "4.0.0",
    "eslint-plugin-sort-keys-fix": "1.0.1",
    "husky": "1.3.1",
    "hygen": "4.0.9",
    "lint-staged": "8.2.1",
    "prettier": "1.18.2",
    "prettier-eslint": "8.8.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "sort-package-json": "1.22.1",
    "storybook-chromatic": "1.4.0",
    "theme-ui": "0.2.42"
  }
}
