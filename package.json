{
  "name": "@socialgouv/emjpm-ui",
  "version": "0.0.1",
  "description": "Emjpm design system",
  "repository": "git@github.com:SocialGouv/emjpm-ui.git",
  "author": "kwiss <christophe@hooraylabs.com>",
  "license": "Apache-2.0",
  "files": [
    "dist"
  ],
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "lerna run build",
    "build-docs": "build-storybook --docs",
    "build-storybook": "build-storybook",
    "chromatic": "CHROMATIC_APP_CODE=3zckn0l8m8w chromatic test",
    "lint": "eslint .",
    "lint:js": "cross-env NODE_ENV=production eslint --cache --cache-location=.cache/eslint --ext .js,.jsx,.html,.ts,.tsx,.mjs --report-unused-disable-directives",
    "lint:package": "sort-package-json",
    "storybook": "start-storybook -p 6006",
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "yarn lint:js --fix",
        "git add"
      ],
      "*.html": [
        "yarn lint:js --fix",
        "git add"
      ],
      "package.json": [
        "yarn lint:package",
        "git add"
      ]
    }
  },
  "dependencies": {
    "lerna": "^3.16.4",
    "polished": "^3.3.0",
    "rebass": "^4.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@socialgouv/emjpm-ui-global-style": "1.1.0",
    "@socialgouv/emjpm-ui-theme": "1.1.0",
    "@emotion/core": "^10.0.15",
    "@emotion/styled": "^10.0.15",
    "@storybook/addon-a11y": "^5.2.0-beta.23",
    "@storybook/addon-actions": "^5.2.0-beta.23",
    "@storybook/addon-docs": "^5.2.0-beta.23",
    "@storybook/addon-knobs": "^5.2.0-beta.23",
    "@storybook/addon-storysource": "^5.2.0-beta.23",
    "@storybook/addons": "^5.2.0-beta.23",
    "@storybook/react": "^5.2.0-beta.23",
    "@storybook/source-loader": "^5.2.0-beta.23",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "cross-env": "^5.1.4",
    "eslint": "^5.0.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4",
    "prettier-eslint": "^8.8.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "sort-package-json": "^1.21.0",
    "storybook-chromatic": "^1.3.3",
    "theme-ui": "^0.2.31"
  }
}
